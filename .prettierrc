{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "all",
	"semi": true,
	"bracketSpacing": true,
	"printWidth": 100,
	"arrowParens": "avoid",
	"bracketSameLine": false,

	"plugins": ["prettier-plugin-svelte"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
